{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-body-style": 0,
    "babel/semi": 2,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  }
}
